{
  "name": "failx/templates-jungerkammerchor",
  "type": "typo3-cms-extension",
  "description": "",
  "keywords": [
    "TYPO3 CMS",
    "Extension"
  ],
  "license": [
    "GPL-3.0-or-later"
  ],
  "require": {
    "typo3/cms-core": ">=8.7",
    "gridelementsteam/gridelements": ">=8.4",
    "mask/mask": "^3.3"
  },
  "autoload": {
    "psr-4": {
      "Failx\\FxTemplatesJungerkammerchor\\": "Classes/"
    }
  },
  "replace": {
    "fx_templates_jungerkammerchor": "self.version",
    "typo3-ter/fx-templates": "self.version"
  },
  "scripts": {
    "post-autoload-dump": [
      "mkdir -p .Build/Web/typo3conf/ext/",
      "[ -L .Build/Web/typo3conf/ext/fx_templates_jungerkammerchor ] || ln -snvf ../../../../. .Build/Web/typo3conf/ext/fx_templates_jungerkammerchor"
    ]
  },
  "config": {
    "vendor-dir": ".Build/vendor",
    "bin-dir": ".Build/bin",
    "platform": {
      "php": "7.0"
    }
  },
  "extra": {
    "typo3/cms": {
      "extension-key": "fx_templates_jungerkammerchor",
      "cms-package-dir": "{$vendor-dir}/typo3/cms",
      "web-dir": ".Build/Web"
    }
  }
}
