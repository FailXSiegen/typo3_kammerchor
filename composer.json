{
    "name": "riconet/t3-project-jungerkammerchorsiegen",
    "description": "t3-project-customer",
    "license": "GPL-3.0-or-later",
    "repositories": [
        {
             "type": "composer",
             "url": "https://composer.typo3.org/",
             "options": {
                "ssl": {
                    "verify_peer": "false"
                }
            }
        },
        {
            "type": "path",
            "url": "extensions/*",
            "options": {
                "symlink": true
            }
        }
    ],
    "config": {
        "platform": {
            "php": "7.4.29"
        },
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true,
            "helhum/dotenv-connector": true
        }
    },
    "require": {
        "php": "^7.4",
        "typo3/cms-backend": "^11.5",
        "typo3/cms-core": "^11.5",
        "typo3/cms-extbase": "^11.5",
        "typo3/cms-extensionmanager": "^11.5",
        "typo3/cms-filelist": "^11.5",
        "typo3/cms-fluid": "^11.5",
        "typo3/cms-frontend": "^11.5",
        "typo3/cms-install": "^11.5",
        "typo3/cms-recordlist": "^11.5",
        "typo3/cms-about": "^11.5",
        "typo3/cms-belog": "^11.5",
        "typo3/cms-beuser": "^11.5",
        "typo3/cms-felogin": "^11.5",
        "typo3/cms-fluid-styled-content": "^11.5",
        "typo3/cms-form": "^11.5",
        "typo3/cms-impexp": "^11.5",
        "typo3/cms-info": "^11.5",
        "typo3/cms-rte-ckeditor": "^11.5",
        "typo3/cms-setup": "^11.5",
        "typo3/cms-seo": "^11.5",
        "typo3/cms-sys-note": "^11.5",
        "typo3/cms-t3editor": "^11.5",
        "typo3/cms-tstemplate": "^11.5",
        "typo3/cms-viewpage": "^11.5",
        "typo3/cms-redirects": "^11.5",
        "typo3/cms-lowlevel": "^11.5",
        "typo3/cms-recycler": "^11.5",
        "typo3/cms-dashboard": "^11.5",
        "helhum/typo3-console": "^7.0",
        "helhum/dotenv-connector": "^2.1",
        "gridelementsteam/gridelements":"^11.0",
        "mask/mask": "^7.0",
        "t3/min": "^1.7",
        "lochmueller/staticfilecache": "^12",
        "plan2net/webp": "^4",
        "jigal/t3adminer":"^11",
        "failx/templates-jungerkammerchor": "@dev"
    },
    "require-dev": {
		"deployer/deployer": "^6",
		"deployer/recipes": "^6"
    },
    "extra": {
        "typo3/cms": {
            "cms-package-dir": "{$vendor-dir}/typo3/cms",
            "web-dir": "public"
        }
    }
}
